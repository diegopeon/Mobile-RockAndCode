<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_1_tela_02_background"
    tools:context=".tela02_intro01">


    <ImageView
        android:id="@+id/logo_01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        android:contentDescription="logo_01"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/_2_tela_02_logo" />

    <ImageView
        android:id="@+id/rockncode1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="rockncode1"
        app:layout_constraintBottom_toTopOf="@+id/desc1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo_01"
        app:srcCompat="@drawable/_3_tela_02_nome" />

    <ImageView
        android:id="@+id/desc1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="desc1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo_01"
        app:srcCompat="@drawable/_4_tela_02_descricao" />
</androidx.constraintlayout.widget.ConstraintLayout>
